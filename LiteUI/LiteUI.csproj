    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>LambdaCom</Authors>
    <Copyright>LambdaCom</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Custom interface styles and Android-like page navigation windows for WPF.</Description>
    <PackageLicenseFile></PackageLicenseFile>
    <Product>LiteUI</Product>
