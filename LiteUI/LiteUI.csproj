  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>D:\ptr22\Source\VisualStudio\LiteUI\LiteUI\LiteUI.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="LiteUI.xml" />
  </ItemGroup>

  <ItemGroup>
